{
  "type": "module",
  "name": "@kosmojs/dev",
  "version": "0.0.6",
  "cacheVersion": "001",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "pkg/*"
  ],
  "bin": {
    "kosmo": "pkg/cli/cli.js"
  },
  "exports": {
    ".": {
      "types": "./pkg/src/index.d.ts",
      "default": "./pkg/index.js"
    },
    "./routes": {
      "types": "./pkg/src/base-plugin/routes.d.ts",
      "default": "./pkg/base-plugin/routes.js"
    },
    "./stub-generator": {
      "types": "./pkg/src/stub-generator/index.d.ts",
      "default": "./pkg/stub-generator/index.js"
    },
    "./cli": {
      "types": "./pkg/src/cli/index.d.ts",
      "default": "./pkg/cli/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "esbuilder src/index.ts src/base-plugin/routes.ts src/base-plugin/worker.ts src/stub-generator/index.ts src/cli/cli.ts src/cli/index.ts",
    "postbuild": "rsync -av --delete src/cli/templates/ pkg/cli/templates",
    "test": "vitest --root ../../.. --project core/dev"
  },
  "dependencies": {
    "@kosmojs/api": "workspace:^",
    "@kosmojs/api-generator": "workspace:^",
    "@kosmojs/devlib": "workspace:^",
    "@kosmojs/fetch-generator": "workspace:^",
    "chokidar": "^4.0.3",
    "crc": "^4.3.2",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.0",
    "fs-extra": "^11.3.2",
    "ora": "^9.0.0",
    "picomatch": "^4.0.3",
    "tfusion": "^0.0.23",
    "tinyglobby": "^0.2.15",
    "ts-morph": "^27.0.2",
    "typescript": "^5.9.3",
    "concurrently": "^9.2.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/koa": "^3.0.1",
    "@types/picomatch": "^4.0.2",
    "vite": "^7.2.2",
    "@kosmojs/config": "workspace:^",
    "@solidjs/router": "^0.15.4",
    "@types/koa__router": "^12.0.5",
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "qs": "^6.14.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.5",
    "solid-js": "^1.9.10",
    "tslib": "^2.8.1",
    "vite-plugin-solid": "^2.11.10"
  }
}
