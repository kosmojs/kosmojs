    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "esbuild": "^0.27.0",
    "globby": "^15.0.0",
    "qs": "^6.14.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.5",
    "solid-js": "^1.9.10",
